{
  "name": "@itavy/mq-router",
  "version": "0.0.0",
  "description": "MQ Router",
  "main": "index.js",
  "scripts": {
    "test": "./node_modules/.bin/mocha",
    "test:coverage": "./node_modules/.bin/nyc npm test",
    "test:check-style": "./node_modules/.bin/eslint index.js lib/latest/* \"test/**/*.js\"",
    "test:e2e": "npm run test:e2e:rabbitmq",
    "test:e2e:rabbitmq": "./node_modules/.bin/tap -b -t 5 ./test/e2e/*.js",
    "build": "npm run build:v6x",
    "build:add": "git add lib",
    "build:v6x": "BABEL_ENV=v6x ./node_modules/.bin/babel lib/latest -d lib/v6x",
    "pretest": "npm run test:check-style",
    "generate-jsdoc": "./node_modules/.bin/jsdoc -c jsdoc.conf.json"
  },
  "pre-commit": [
    "test",
    "build",
    "build:add"
  ],
  "repository": {
    "type": "git",
    "url": "git+ssh://git@bitbucket.org/itavy/mq-router.git"
  },
  "author": "Octavian Ionescu <itavyg@gmail.com>",
  "license": "ISC",
  "homepage": "https://bitbucket.org/itavy/mq-router#readme",
  "devDependencies": {
    "@itavy/test-utilities": "^4.1.0",
    "babel-cli": "^6.26.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.0",
    "eslint": "^4.15.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.5.1",
    "eyes": "^0.1.8",
    "ink-docstrap": "^1.3.2",
    "jaguarjs-jsdoc": "^1.0.2",
    "jsdoc": "^3.4.3",
    "jsdoc-to-markdown": "^3.0.3",
    "mocha": "^4.1.0",
    "nyc": "^11.4.1",
    "pre-commit": "^1.2.2",
    "tap": "~11.1.3"
  },
  "dependencies": {
    "@itavy/ierror": "~1.1.2",
    "@itavy/mq-connector": "git+ssh://git@bitbucket.org/itavy/mq-connector.git#initial_release",
    "@itavy/mq-structure": "git+ssh://git@bitbucket.org/itavy/mq-structure.git#initial_release",
    "debug": "~3.1.0",
    "semver": "~5.4.1",
    "uuid": "~3.1.0"
  },
  "nyc": {
    "all": true,
    "color:": true,
    "check-coverage": false,
    "per-file": true,
    "lines": 95,
    "statements": 95,
    "functions": 95,
    "branches": 95,
    "include": [
      "lib/latest/*.js"
    ],
    "reporter": [
      "lcov",
      "text"
    ],
    "watermarks": {
      "check-coverage": true,
      "statements": [
        50,
        95
      ],
      "lines": [
        50,
        95
      ],
      "functions": [
        50,
        95
      ],
      "branches": [
        50,
        95
      ]
    }
  }
}
