{
  "name": "mq-router",
  "version": "0.0.0",
  "description": "MQ Router",
  "main": "index.js",
  "scripts": {
    "test": "./node_modules/.bin/mocha",
    "test:coverage": "./node_modules/.bin/nyc npm test",
    "test:check-style": "./node_modules/.bin/eslint index.js lib/* \"test/**/*.js\"",
    "pretest": "npm run test:check-style",
    "generate-jsdoc": "./node_modules/.bin/jsdoc -c jsdoc.conf.json"
  },
  "pre-commit": [
    "test"
  ],
  "repository": {
    "type": "git",
    "url": "git+ssh://git@bitbucket.org/itavy/mq-router.git"
  },
  "author": "Octavian Ionescu <itavyg@gmail.com>",
  "license": "ISC",
  "homepage": "https://bitbucket.org/itavy/mq-router#readme",
  "devDependencies": {
    "@itavy/test-utilities": "~4.0.0",
    "eslint": "^4.8.0",
    "eslint-config-airbnb": "^16.0.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.4.0",
    "eyes": "^0.1.8",
    "ink-docstrap": "~1.3.0",
    "jaguarjs-jsdoc": "^1.0.2",
    "jsdoc": "^3.4.3",
    "jsdoc-to-markdown": "^3.0.0",
    "mocha": "~4.0.1",
    "nyc": "~11.2.1",
    "pre-commit": "^1.2.2",
    "tap": "~10.7.2"
  },
  "dependencies": {
    "@itavy/ierror": "~1.1.2",
    "@itavy/mq-connector": "git+ssh://git@bitbucket.org/itavy/mq-connector.git#initial_release",
    "@itavy/mq-structure": "git+ssh://git@bitbucket.org/itavy/mq-structure.git#initial_release",
    "@itavy/simple-timer": "~1.0.2",
    "@itavy/utilities": "~1.1.1",
    "debug": "~3.1.0",
    "uuid": "~3.1.0"
  },
  "nyc": {
    "all": true,
    "color:": true,
    "check-coverage": true,
    "per-file": true,
    "lines": 95,
    "statements": 95,
    "functions": 95,
    "branches": 95,
    "include": [
      "lib/**/*.js"
    ],
    "reporter": [
      "lcov",
      "text"
    ],
    "watermarks": {
      "check-coverage": true,
      "statements": [
        50,
        95
      ],
      "lines": [
        50,
        95
      ],
      "functions": [
        50,
        95
      ],
      "branches": [
        50,
        95
      ]
    }
  }
}
